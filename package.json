{
  "name": "ammo-worker",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "types": "./src/main.ts",
  "exports": {
    ".": "./src/main.ts",
    "./types": "./src/types.ts",
    "./adapters": "./src/adapters",
    "./debug": "./src/debug"
  },
  "scripts": {
    "start": "vite --host",
    "build": "vite build --base=/ammo-worker/",
    "postbuild": "node ./scripts/postbuild",
    "build-ammo": "rollup -c ./scripts/rollup.config.js --environment PROD",
    "preview": "vite preview",
    "check": "tsc"
  },
  "dependencies": {
    "comlink": "^4.3.1"
  },
  "devDependencies": {
    "@react-three/drei": "^9.19.6",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-replace": "^4.0.0",
    "@tweakpane/core": "^1.1.0",
    "@types/ammo.js": "github:osman-turan/ammo.js-typings",
    "@types/gl-matrix": "^3.2.0",
    "@types/three": "^0.143.0",
    "ammo.js": "github:kripken/ammo.js",
    "gl-matrix": "^3.4.3",
    "nice-color-palettes": "^2.0.0",
    "postcss": "^8.4.14",
    "postprocessing": "^6.28.5",
    "rollup": "^2.77.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.1.8",
    "three": "^0.143.0",
    "three-kit": "github:michealparks/three-kit",
    "tslib": "^2.4.0",
    "tweakpane": "^3.1.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  }
}
