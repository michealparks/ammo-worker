{
  "name": "ammo-worker",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "main": "./src/main.ts",
  "scripts": {
    "start": "vite --host",
    "build": "vite build --base=/ammo-worker/",
    "postbuild": "node ./scripts/postbuild",
    "build-ammo": "rollup -c ./scripts/rollup.config.js --environment PROD",
    "preview": "vite preview",
    "check": "tsc",
    "check-playground": "svelte-check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "comlink": "^4.3.1"
  },
  "devDependencies": {
    "nice-color-palettes": "^3.0.0",
    "rollup": "^2.77.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "three": "^0.142.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "@react-three/drei": "^9.17.3",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-replace": "^4.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/ammo.js": "github:osman-turan/ammo.js-typings",
    "@types/three": "^0.142.0",
    "ammo.js": "github:kripken/ammo.js",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "postcss": "^8.4.14",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.6"
  }
}
